{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": [
        "framework",
        "laravel",
        "lumen"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2",
        "darkaonline/swagger-lume": "7.*",
        "doctrine/dbal": "^2.10",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^6.5",
        "illuminate/mail": "^7.0",
        "laravel/lumen-framework": "^7.0",
        "lcobucci/jwt": "3.3.3",
        "spatie/laravel-fractal": "5.7.0",
        "spatie/laravel-query-builder": "^2.8",
        "tucker-eric/eloquentfilter": "^2.3",
        "tymon/jwt-auth": "1.0.0",
        "vlucas/phpdotenv": "^4.0"
    },
    "require-dev": {
        "flipbox/lumen-generator": "^6.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "bootstrap/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
